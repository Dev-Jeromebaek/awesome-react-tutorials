{
    "name": "awesome-react-redux-skeleton",
    "version": "1.0.0",
    "description": "# No Create-React-App",
    "main": "index.js",
    "scripts": {
        "clean": "rm -rf dist && webpack --mode production",
        "build": "webpack --mode production",
        "start": "echo \"start ./index.html\" && webpack-dev-server --config ./webpack.config.js --mode development",
        "dev": "echo \"start dist/index.html\" && webpack-dev-server --config ./webpack.config.dev.js --mode development",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": "git+https://github.com/Dev-JeromeBaek/awesome-react-redux-skeleton.git",
    "keywords": [],
    "author": "Dev-JeromeBaek <superior051@icloud.com>",
    "license": "MIT",
    "babel": {
        "presets": [
            "@babel/preset-env",
            "@babel/preset-react"
        ],
        "plugins": [
            "@babel/plugin-syntax-dynamic-import",
            "@babel/plugin-syntax-import-meta",
            "@babel/plugin-proposal-class-properties",
            "@babel/plugin-proposal-json-strings",
            [
                "@babel/plugin-proposal-decorators",
                {
                    "legacy": true
                }
            ],
            "@babel/plugin-proposal-function-sent",
            "@babel/plugin-proposal-export-namespace-from",
            "@babel/plugin-proposal-numeric-separator",
            "@babel/plugin-proposal-throw-expressions"
        ]
    },
    "bugs": {
        "url": "https://github.com/Dev-JeromeBaek/awesome-react-redux-skeleton/issues"
    },
    "homepage": "https://github.com/Dev-JeromeBaek/awesome-react-redux-skeleton#readme",
    "devDependencies": {
        "@babel/core": "^7.0.0",
        "@babel/plugin-proposal-class-properties": "^7.0.0",
        "@babel/plugin-proposal-decorators": "^7.0.0",
        "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
        "@babel/plugin-proposal-function-sent": "^7.0.0",
        "@babel/plugin-proposal-json-strings": "^7.0.0",
        "@babel/plugin-proposal-numeric-separator": "^7.0.0",
        "@babel/plugin-proposal-throw-expressions": "^7.0.0",
        "@babel/plugin-syntax-dynamic-import": "^7.0.0",
        "@babel/plugin-syntax-import-meta": "^7.0.0",
        "@babel/preset-env": "^7.0.0",
        "@babel/preset-react": "^7.0.0",
        "babel-eslint": "^10.0.1",
        "babel-loader": "^8.0.0",
        "css-loader": "^1.0.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-jsx-a11y": "^6.1.2",
        "eslint-plugin-react": "^7.11.1",
        "file-loader": "^2.0.0",
        "html-webpack-plugin": "^3.2.0",
        "style-loader": "^0.23.0",
        "uglifyjs-webpack-plugin": "^1.3.0",
        "url-loader": "^1.1.1",
        "webpack": "^4.17.1",
        "webpack-cli": "^3.1.0",
        "webpack-dev-server": "^3.1.6"
    },
    "dependencies": {
        "prop-types": "^15.6.2",
        "react": "^16.4.2",
        "react-dom": "^16.4.2",
        "react-redux": "^5.0.7",
        "redux": "^4.0.0"
    }
}
